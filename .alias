# ALIASES
#
# NVIM
alias vi="nvim"
alias vim="nvim"

# Git
alias gs="git status"
alias gpom-rb="git pull origin master --rebase"
alias ga="git add ."
alias gaa="git add -A"
alias gc="git commit -m $1"
alias gl="git log"
alias gb="git checkout -b $1"
alias gco="git checkout $1"
alias gwho="git config --global user.email"
alias gorigin="git config remote.origin.url"

gsetorigin() {
	host=$1
	repo=$2
	echo "git remote set-url origin git@github.com$host:$repo"
}
gpo() {
  branch=`git branch | grep \* | cut -d ' ' -f2`
  git push origin $branch $1
}
grebase () {
  num=$1
  git rebase -i HEAD~$num
}

# KeePassX
alias kee="sed -n 's/^keepass=//p' ~/.keepass | pbcopy"

# GO FRESH ALIAS
alias f="fresh"

# Tmux
alias t=tmux
